<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent" />
            <size android:width="24dp" android:height="24dp" />
        </shape>
    </item>
    <item android:left="4dp" android:top="6dp" android:right="16dp" android:bottom="6dp">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <size android:height="12dp" android:width="4dp" />
            <solid android:color="@color/indicatorColor" />
        </shape>
    </item>
    <item android:left="10dp" android:top="4dp" android:right="10dp" android:bottom="4dp">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <size android:height="16dp" android:width="4dp" />
            <solid android:color="@color/indicatorColor" />
        </shape>
    </item>
    <item android:left="16dp" android:top="6dp" android:right="4dp" android:bottom="6dp">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <size android:height="12dp" android:width="4dp" />
            <solid android:color="@color/indicatorColor" />
        </shape>
    </item>
</layer-list>
